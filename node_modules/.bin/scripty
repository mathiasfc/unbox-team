#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/unbox/Documents/unbox/yggdrasil/node_modules/.pnpm/scripty@2.0.0/node_modules/scripty/cli.js/node_modules:/home/unbox/Documents/unbox/yggdrasil/node_modules/.pnpm/scripty@2.0.0/node_modules/scripty/node_modules:/home/unbox/Documents/unbox/yggdrasil/node_modules/.pnpm/scripty@2.0.0/node_modules:/home/unbox/Documents/unbox/yggdrasil/node_modules/.pnpm/node_modules:/home/unbox/Documents/unbox/yggdrasil/node_modules:/home/unbox/Documents/unbox/node_modules:/home/unbox/Documents/node_modules:/home/unbox/node_modules:/home/node_modules:/node_modules:/home/unbox/Documents/unbox/yggdrasil/applications/team/node_modules:/home/unbox/Documents/unbox/yggdrasil/applications/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/scripty@2.0.0/node_modules/scripty/cli.js" "$@"
else 
  exec node  "$basedir/../../../../node_modules/.pnpm/scripty@2.0.0/node_modules/scripty/cli.js" "$@"
fi
